{
  "main": "dist/index.js",
  "types": "module",
  "dependencies": {
    "@types/cors": "^2.8.12",
    "@types/dotenv": "^8.2.0",
    "@types/ejs": "^3.1.0",
    "@types/express": "^4.17.13",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/sequelize": "^4.28.11",
    "colors": "^1.4.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "ejs": "^3.1.6",
    "express": "^4.17.3",
    "jsonwebtoken": "^8.5.1",
    "nodemon": "^2.0.15",
    "pg": "^8.7.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.17.0",
    "typescript": "^4.6.3"
  },
  "scripts": {
    "start": "nodemon dist/index.js",
    "copy-files": "cp -r ./views ./dist",
    "migration": "npx sequelize-cli db:migrate --migrations-path dist/db/migrations/create",
    "server": "tsc -w & yarn copy-files && nodemon dist/index.js",
    "build": "tsc && yarn copy-files && yarn migration",
    "prepare": "cd .. && husky install server/.husky",
    "format": "pretty-quick --staged"
  },
  "devDependencies": {
    "husky": "^7.0.4",
    "prettier": "^2.6.2",
    "pretty-quick": "^3.1.3",
    "sequelize-cli": "^6.4.1"
  }
}
